{"version":"0.3.0","body":"function main(workbook: ExcelScript.Workbook) {\n    const sheet = workbook.getActiveWorksheet();\n\n    // Lista completa de substituições para português\n    const pares = [\n        // A / a\n        [\"Á\", \"A\"], [\"À\", \"A\"], [\"Â\", \"A\"], [\"Ã\", \"A\"], [\"Ä\", \"A\"],\n        [\"á\", \"a\"], [\"à\", \"a\"], [\"â\", \"a\"], [\"ã\", \"a\"], [\"ä\", \"a\"],\n\n        // E / e\n        [\"É\", \"E\"], [\"È\", \"E\"], [\"Ê\", \"E\"], [\"Ë\", \"E\"],\n        [\"é\", \"e\"], [\"è\", \"e\"], [\"ê\", \"e\"], [\"ë\", \"e\"],\n\n        // I / i\n        [\"Í\", \"I\"], [\"Ì\", \"I\"], [\"Î\", \"I\"], [\"Ï\", \"I\"],\n        [\"í\", \"i\"], [\"ì\", \"i\"], [\"î\", \"i\"], [\"ï\", \"i\"],\n\n        // O / o\n        [\"Ó\", \"O\"], [\"Ò\", \"O\"], [\"Ô\", \"O\"], [\"Õ\", \"O\"], [\"Ö\", \"O\"],\n        [\"ó\", \"o\"], [\"ò\", \"o\"], [\"ô\", \"o\"], [\"õ\", \"o\"], [\"ö\", \"o\"],\n\n        // U / u\n        [\"Ú\", \"U\"], [\"Ù\", \"U\"], [\"Û\", \"U\"], [\"Ü\", \"U\"],\n        [\"ú\", \"u\"], [\"ù\", \"u\"], [\"û\", \"u\"], [\"ü\", \"u\"],\n\n        // Cedilha\n        [\"Ç\", \"C\"],\n        [\"ç\", \"c\"]\n    ];\n\n    // Colunas específicas onde você quer aplicar a limpeza\n    const colunas = [\"A:A\", \"B:B\", \"D:D\"];\n\n    // Para cada coluna, aplicar cada substituição\n    colunas.forEach(coluna => {\n        const range = sheet.getRange(coluna);\n        pares.forEach(par => {\n            range.replaceAll(par[0], par[1], {\n                completeMatch: false,\n                matchCase: true  // mantém maiúscula/minúscula certinho\n            });\n        });\n    });\n}","description":"","noCodeMetadata":"","parameterInfo":"{\"version\":1,\"originalParameterOrder\":[],\"parameterSchema\":{\"type\":\"object\",\"default\":{},\"x-ms-visibility\":\"internal\"},\"returnSchema\":{\"type\":\"object\",\"properties\":{}},\"signature\":{\"comment\":\"\",\"parameters\":[{\"name\":\"workbook\",\"comment\":\"\"}]}}","apiInfo":"{\"variant\":\"synchronous\",\"variantVersion\":2}"}